 8000880:	681b      	ldr	r3, [r3, #0]
 8000882:	4a0e      	ldr	r2, [pc, #56]	; (80008bc <ITM_SendChar+0x48>)
 8000884:	f043 7380 	orr.w	r3, r3, #16777216	; 0x1000000
 8000888:	6013      	str	r3, [r2, #0]

	//enable stimulus port 0
	ITM_TRACE_EN |= ( 1 << 0);
